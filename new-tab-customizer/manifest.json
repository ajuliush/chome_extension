{
    "manifest_version": 3,
    "name": "New Tab Customizer",
    "version": "1.0",
    "description": "A simple extension to customize your new tab page with a clock, quotes, and more.",
    "chrome_url_overrides": {
      "newtab": "newtab.html"
    },
    "icons": {
      "16": "assets/icon16.png",
      "48": "assets/icon48.png",
      "128": "assets/icon128.png"
    },
    "permissions": [
      "storage"
    ]
  }
  